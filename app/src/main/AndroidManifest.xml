<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.shayari">
<uses-permission android:name="android.permission.INTERNET">

</uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cateogry.Happy" android:label="Happy"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".cateogry.Motivational" android:label="Motivational"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".cateogry.Sad" android:label="Sad"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".cateogry.Romantic" android:label="Romantic"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>